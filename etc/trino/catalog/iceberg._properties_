connector.name=iceberg
iceberg.catalog.type=jdbc
iceberg.jdbc-catalog.default-warehouse-dir=s3a://warehouse/
iceberg.jdbc-catalog.connection-url=jdbc:comdb2://comdb2/icebergdb?comdb2dbname=icebergdb
iceberg.jdbc-catalog.driver-class=com.bloomberg.comdb2.jdbc.Driver
iceberg.jdbc-catalog.catalog-name=iceberg
iceberg.file-format=PARQUET
hive.s3.aws-access-key=${ENV:AWS_ACCESS_KEY}
hive.s3.aws-secret-key=${ENV:AWS_SECRET_KEY}
hive.s3.endpoint=${ENV:AWS_ENDPOINT}
hive.s3.path-style-access=true
